{
  "name": "performance-fix",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "overrides": {
    "@ag-ui/client": "0.0.45"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:agent\" --names ui,agent --prefix-colors blue,green --kill-others",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "dev:agent": "cd agent-server && npm run build && node dist/index.js",
    "build:agent": "cd agent-server && npm run build",
    "dev:ui": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ."
  },
  "dependencies": {
    "@a2a-js/sdk": "0.2.5",
    "@a2ui/lit": "^0.8.1",
    "@ag-ui/a2a": "0.0.6",
    "@ag-ui/client": "0.0.45",
    "@copilotkit/a2ui-renderer": "^1.51.3",
    "@copilotkit/react-core": "1.51.3",
    "@copilotkit/runtime": "1.51.3",
    "hono": "^4.6.18",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.34.0",
    "knip": "^5.83.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.2"
  }
}
